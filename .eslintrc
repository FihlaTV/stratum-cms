{
	"env": {
		"node": true
	},
	"extends": [
		"keystone-react",
		"prettier"
	],
	"rules": {
		"react/jsx-no-bind": 0,
		"react/jsx-pascal-case": ["error", {"allowAllCaps": true}]
	},
	"globals": {
		"_": true,
		"$": true,
		"after": true,
		"afterEach": true,
		"before": true,
		"beforeEach": true,
		"describe": true,
		"it": true,
		"jQuery": true,
		"Keystone": true,
		"test": true,
		"expect": true
	}
}

