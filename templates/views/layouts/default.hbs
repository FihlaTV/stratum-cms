<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<title>{{{getTitle breadcrumbs brand ' > '}}}</title>
		<link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
		
		{{#ifeq env "production"}}
		<link href="{{styleEntry}}" rel="stylesheet">
		{{/ifeq}}

		{{!-- This file provides the default styling for the KeystoneJS Content Editor
			- only renders when user is logged in and admin--}}
		{{{isAdminEditorCSS user}}}

		<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
		<!--[if lt IE 9]>
		  <script src="//oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
		  <script src="//oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
		<![endif]-->

	</head>
	<body>
		<div id="header"{{#ifeq section 'homex'}} class="header-first"{{/ifeq}}>
			{{!-- <div class="container"> --}}
				<div role="navigation" class="navbar navbar-big navbar-default navbar-static-top">
					<div class="navbar-header">
						<div class="container">
							<button type="button" data-toggle="collapse" data-target=".navbar-collapse" class="navbar-toggle">
								<span class="sr-only">Toggle navigation</span>
								<span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
							</button>
							<a href="/" class="navbar-brand">
								<img src="/images/logo_menu_big.png" alt="{{brand}}" class="navbar-brand-image-big">
								<img src="/images/logo_menu_small.png" alt="{{brand}}" class="navbar-brand-image-small">
							</a>
							{{!-- <a href="/" class="navbar-brand">{{#if brand}}{{brand}}{{else}}Stratum<sup>3</sup>{{/if}}</a> --}}
						</div>
					</div>
					<div class="navbar-main">
						<div class="container">
							<div class="collapse navbar-collapse">
								<ul class="nav navbar-nav navbar-left" role="menu">
                                {{# each navLinks}}
                                    <li class="{{#ifeq ../section key}}active{{#if ../data.page.hasSubPages}} dropdown{{/if}}{{/ifeq}}">
                                        {{#ifeq ../section key}}
                                        {{#unless ../data.pages}}
                                        <a href="{{ href }}">{{ label }}</a>
                                        {{else}}
                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">{{ label }} <span class="caret"></span></a>
                                        <ul class="dropdown-menu">
                                        {{/unless}}
                                        {{#each ../data.pages}}
                                            {{#ifeq ../../data.menuPage.slug slug}}
                                            <li class="{{#ifeq ../../data.page.slug slug}}active {{/ifeq}}{{#if this.hasSubPages}}has-sub-pages expanded{{/if}}">
                                                <a href="/{{../../data.currentMenuBlock.slug}}/{{slug}}">{{titleForMenu}}</a>
                                            </li>
                                            {{#each ../../data.subPages}}
                                                <li class="sub-sub-nav{{#ifeq ../../../data.page.slug slug}} active{{/ifeq}}"><a href="/{{../../../data.currentMenuBlock.slug}}/{{slug}}">{{titleForMenu}}</a></li>
                                            {{/each}}
                                            {{else}}
                                            <li {{#if hasSubPages}}class="has-sub-pages"{{/if}}>
                                                <a href="/{{../../data.currentMenuBlock.slug}}/{{slug}}">{{titleForMenu}}</a>
                                            </li>
                                            {{/ifeq}}
                                        {{/each}}
                                        {{#if ../data.pages}}
                                        </ul>
                                        {{/if}}
                                        {{else}}
                                        <a href="{{ href }}">{{ label }}</a>
                                        {{/ifeq}}
                                    </li>
                                {{/each}}
								</ul>
								<ul class="nav navbar-nav navbar-right">
									<li class="search">
										<a href="#search"></a>
									</li>
								</ul>
								{{#if stratumUser}}
								<ul class="nav navbar-nav navbar-right">
									<li>
										<a href="/logout">Logga ut ({{stratumUser.FirstName}} {{stratumUser.LastName}})</a>
									</li>
								</ul>
								{{else}}
								<div id="login-button-react"></div>
								{{/if}}
							</div>
						</div>
					</div>
				{{!-- </div> --}}
			</div>
		</div>
		<div id="body">
		{{#if breadcrumbs}}
			<div class="container">
				<ol class="breadcrumb">
					<li><a href="/">Start</a></li>
					{{#each breadcrumbs}}
					{{#if @last}}
					<li class="active">{{label}}</li>
					{{else}}
					<li><a href="{{path}}">{{label}}</a></li>
					{{/if}}
					{{/each}}
				</ol>
			</div>
		{{/if}}
			<!--
				Flash messages allow you to display once-off status messages to users, e.g. form
				validation errors, success messages, etc
			-->
			{{#if messages}}
			<div id="flash-messages" class="container">
				{{#if messages.info}}
				<div class="alert alert-info">
						{{{flashMessages messages.info}}}
				</div>
				{{/if}}
				{{#if messages.success}}
				<div class="alert alert-success">
						{{{flashMessages messages.success}}}
				</div>
				{{/if}}
				{{#if messages.warning}}
				<div class="alert alert-warning">
						{{{flashMessages messages.warning}}}
				</div>
				{{/if}}
				{{#if messages.error}}
				<div class="alert alert-danger">
						{{{flashMessages messages.error}}}
				</div>
				{{/if}}
			</div>
			{{/if}}
			<!-- The content block should contain the body of your template's content-->
			{{{body}}}
		</div>
		<footer>
			<div class="container">
			{{#if lastCommit}}
			{{#with lastCommit.commit}}
				<div id="dev-info" class="hidden-xs" style="float: right; text-align: right; margin-bottom: 10px;">
					<img src="{{author.avatar_url}}&s=40" width="20" height="20" class="img-rounded"> <a target="_blank" href="{{author.html_url}}">{{author.login}}</a> authored {{date commit.author.date locale="en" timeago=true}}<br>
					branch {{/with}}<a target="_blank" href="{{lastCommit._links.html}}">{{lastCommit.name}}</a>, 
					{{#with lastCommit.commit}}latest commit <a target="_blank" href="{{html_url}}">{{shortenString sha 10}}</a><br>
				</div>
			{{/with}}
			{{/if}}
			{{address}}
				{{!-- <p>Powered by <a href="http://keystonejs.com" target="_blank">KeystoneJS</a>.</p> --}}
			</div>
		</footer>
		{{!--
			//- jQuery 1.11.1 is included by default, and required by both Bootstrap
			//- and the KeystoneJS Content Editor. If you are not targeting IE8,
			//- you can upgrade to 2.1.1 (also bundled) for better performance.
		--}}
		<!--<script src="/js/lib/jquery/jquery-1.11.1.min.js"></script>-->
		{{!--
			//- Customise which Bootstrap javascript files are served by including
			//- them specifically here, instead of bootstrap-3.2.0.min.js
		--}}
		<!--<script src="/js/lib/bootstrap/bootstrap-3.2.0.min.js"></script>-->
		{{!-- React scripts --}}
		{{#ifeq env "development"}}
		<script type="text/javascript" src="/js/bundle.js"></script>
		{{else}}
		<script type="text/javascript" src="/dist/vendors.js"></script>
		<script type="text/javascript" src="/dist/app.bundle.js"></script>
		{{/ifeq}}
		{{#each pageScripts}}
		<script type="text/javascript" src="/js/{{{src}}}"></script>
		{{/each}}
		{{!-- // <script type="text/javascript" src="/js/react/application.js"></script> --}}
		<!--{{{isAdminEditorJS user}}}-->
		{{#if register.showMap}}{{>maps}}{{/if}}
	</body>
</html>
