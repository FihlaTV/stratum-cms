<div class="report-list">
{{#each reports}}
	<div class="col-sm-4 col-xs-12">
		<a href="{{cloudinaryUrl pdf}}" target="_blank" class="">
			<div class="report-list-report">
				<p>{{year}}</p>
				{{{cloudinaryImage pdf width="200" crop="fill" page="1" format="png" class="img-responsive"}}}
			</div>
		</a>
	</div>
{{/each}}
</div>
